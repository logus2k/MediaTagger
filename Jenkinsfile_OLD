pipeline {
    agent {
        docker { 
            image 'mydotnet'
        }
    }
    stages {
        stage('Build') {
            steps {
                sh "dotnet build"
            }
        }
    }
}



        stage('Test') {
            steps {
                bat 'dotnet test --logger trx ./BigProject.NetCoreApp.Test/BigProject.NetCoreApp.Test.csproj'
            }
             post {
                    always {
                      //plugin: https://plugins.jenkins.io/mstest/
                        mstest testResultsFile:"**/*.trx", keepLongStdio: true
                    }
        
                }           
        }

